function SecondSmallest(arr){
    for(let i=0;i<arr.length;i++){
        let mini = [];
        if(arr[i]<mini)
        arr[i]=mini;
        for(let j=0; j<arr.length;j++){
            if(arr[j]>mini,arr[j]<Secmax)
            secmax = i;
            let secmax = [];
        }
    }
    return arr
}
